package  wky{		import flash.filesystem.*;		public class Stats {		public var avg_twait:Number;    	public var max_twait:Number;    	public var avg_tonboard:Number;    	public var max_tonboard:Number;    	public var avg_thput:Number;    	public var max_thput:Number;    	public var avg_load:Number;    	public var avg_dist:Number;    	public var avg_run100:Number;		public function Stats(stream:FileStream) {			avg_twait = stream.readFloat();			max_twait = stream.readFloat();			avg_tonboard = stream.readFloat();			max_tonboard = stream.readFloat();			avg_thput = stream.readFloat();			max_thput = stream.readFloat();			avg_load = stream.readFloat();			avg_dist = stream.readFloat();			avg_run100 = stream.readFloat();		}	}	}