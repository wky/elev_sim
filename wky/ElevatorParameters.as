package wky{	import flash.filesystem.*;		public class ElevatorParameters {		public var elevatorNum:int;		public var elevatorSpeed:int;		public var elevatorLoad:int;		public var levelNum:int;		public var arrivalRate:Number;		public var simulationCycles:int;		public var algorithmSelect:int;		public function ElevatorParameters(stream:FileStream) {			elevatorNum = stream.readInt();			elevatorSpeed = stream.readInt();			elevatorLoad = stream.readInt();			levelNum = stream.readInt();			arrivalRate = stream.readFloat();			simulationCycles = stream.readInt();			algorithmSelect = stream.readInt();		}	}}